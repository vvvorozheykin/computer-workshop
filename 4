#include<iostream>
using namespace std;
int gcd (int a, int b){
    if (b == 0) {
        return a;
    }
    return gcd (b, a%b);
}
int main(){
    int a,b;
    cout << "chislitel:";
    cin >> a;
    cout << "znamenatel:";
    cin >> b;
    int greattestCommonDivisor = gcd(a,b);
    a = a / greattestCommonDivisor;
    b = b / greattestCommonDivisor;
    cout << "sokraschennya drob:" << a << "/" << b << endl;
    return 0;
}
